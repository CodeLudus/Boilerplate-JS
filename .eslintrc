{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": [
          "**/*.config.js" // Permite las dependencias de desarrollo en el archivos de configuraci√≥n como vite.config.js, webpack.config.js, etc.
        ]
      }
    ]
  }
}
